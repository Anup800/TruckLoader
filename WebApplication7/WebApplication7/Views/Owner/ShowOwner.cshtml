@model IEnumerable<WebApplication7.Models.Owner>

@{
    ViewBag.Title = "ShowOWner";
}

<h2>Show Owners</h2>

<p>
    @Html.ActionLink("AddOwner", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Oname)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Opassword)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Ocontact)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Oname)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Opassword)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Ocontact)
            </td>
          
        </tr>
    }

</table>

